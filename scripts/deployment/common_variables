#!/usr/bin/env bash

TRACK="${TRACK:-production}"
APP_NAME="${APP_NAME:-flat}"
KUBE_NAMESPACE="${KUBE_NAMESPACE:-flat}"
MYSQL_USER="${MYSQL_USER:-flat}" 
MYSQL_PASSWORD="${MYSQL_PASSWORD:-flat_mysql_password}" 
MYSQL_DATABASE="${MYSQL_DATABASE:-flat}"
MYSQL_PVC_NAME="${MYSQL_PVC_NAME:-mysql-pvc-$TRACK}"
MYSQL_NAME="${MYSQL_NAME:-mysql-$TRACK}"
PIPELINE="${PIPELINE:-manual-$RANDOM}"
BUILD="${BUILD:-manual-$RANDOM}"
IMAGE_VERSION="${IMAGE_VERSION:-master}"
IMAGE_NAME="${IMAGE_NAME:-registry.gitlab.memleak.pl:443/flat/flat-api}"
DJANGO_SECRET_KEY="${DJANGO_SECRET_KEY:-super_secret_key}"
API_NAME="${API_NAME:-flat-api-$TRACK}"
API_TLS_NAME="${API_TLS_NAME:-$API_NAME-tls}"

echo "TRACK: $TRACK"
echo "APP_NAME: $APP_NAME"
echo "KUBE_NAMESPACE: $KUBE_NAMESPACE"
echo "MYSQL_USER: $MYSQL_USER"
#echo "MYSQL_PASSWORD: $MYSQL_PASSWORD"
echo "MYSQL_DATABASE: $MYSQL_DATABASE"
echo "MYSQL_NAME: $MYSQL_NAME"
echo "MYSQL_PVC_NAME: $MYSQL_PVC_NAME"
echo "PIPELINE: $PIPELINE"
echo "BUILD: $BUILD"
echo "IMAGE_VERSION: $IMAGE_VERSION"
echo "IMAGE_NAME: $IMAGE_NAME"
#echo "DJANGO_SECRET_KEY: $DJANGO_SECRET_KEY"

